<?xml version="1.0" encoding="utf-8" ?>
<widget id="CopyToClipboardButton.widget.CopyToClipboardButton" needsEntityContext="true" xmlns="http://www.mendix.com/widget/1.0/">
    <name>CopyToClipboardButton</name>
    <description>The description of this widget.</description>

    <icon></icon>

    <properties>
        <property key="attrCopy" type="attribute" required="true" isPath="optional" pathType="reference">
            <caption>Attribute</caption>
            <category>General</category>
            <description>The attribute of the context object to copy to clipboard when the user clicks this button.</description>
            <attributeTypes>
                <attributeType name="String"/>
            </attributeTypes>
        </property>
        <property key="appendInput" type="boolean" required="true" defaultValue="false">
            <caption>Append?</caption>
            <category>General</category>
            <description>Choose 'true' to display this button attached to an input. (Must be the immediate sibling of a text-field element, and the two must be the only children of a container element.)</description>
        </property>
        <property key="buttonText" type="string" required="false" defaultValue="">
            <caption>Button text</caption>
            <category>General</category>
            <description>Text to display on the button. Defaults to an image of clipboard.</description>
        </property>
        <property key="buttonClass" type="enumeration" defaultValue="default">
            <caption>Button Class</caption>
            <category>General</category>
            <description>Select a standard styling for the button</description>
            <enumerationValues>
                <enumerationValue key="default">Default</enumerationValue>
                <enumerationValue key="primary">Primary</enumerationValue>
                <enumerationValue key="info">Info</enumerationValue>
                <enumerationValue key="warning">Warning</enumerationValue>
                <enumerationValue key="danger">Danger</enumerationValue>
                <enumerationValue key="inverse">Inverse</enumerationValue>
            </enumerationValues>
        </property>
    </properties>
</widget>
